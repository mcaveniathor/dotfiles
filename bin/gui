#!/bin/sh
mv ~/.vimrc ~/.vimrc_backup
mv ~/.config/i3/config ~/.config/i3/config_backup
mv ~/.zshrc ~/.zshrc_backup
mv ~/.i3status.conf ~/.i3status.conf.backup
cp ~/repos/personal/dotfiles/vim/.vimrc ~/.vimrc
cp ~/repos/personal/dotfiles/i3/config ~/.config/i3/config
cp ~/repos/personal/dotfiles/i3/mpdcurrent.sh ~/.config/i3/mpdcurrent.sh
cp ~/repos/personal/dotfiles/i3/.i3status.conf ~/.i3status.conf
cp ~/repos/personal/dotfiles/zsh/.zshrc ~/.zshrc

